<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/styles.css">

  <title>Hacktiv Game Shop</title>
    <h1>Hacktiv Game Shop</h1>


</head>

<body>
  <!-- your content here... -->
  <a href="/addItem">Add Item</a>
  <table>
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th>Stock</th>
        <th>Category</th>

    </thead>
    <tbody>
        <% result.forEach((el) =>{%>
            <tr class="item-list">
                    <td class="item-name"><%= el.name %></td>
                    <td class="item-price"><%= el.price %> </td>
                    <td class="item-stock"><%= el.stock %></td>
                    <td class="item-category"><%= el.CategoryId %> </td>
                
            </tr>
        <% }) %> 

    </tbody>
</table>
  <script type="text/javascript" src="js/setup.js"></script>
</body>
</html>